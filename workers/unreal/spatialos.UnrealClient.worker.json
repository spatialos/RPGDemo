{
  "build": {
    "tasks_filename": "spatialos.unreal.client.build.json",
	"generated_build_scripts_type": "unreal"
  },
  "bridge": {
    "worker_claim": {
      "claims": [
        "visual"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 2
      }
    },
    "streaming_query": [],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": true
    },
    "asset_context": {
      "use_player_context_if_authoritative": false
    }
  },
  "external": {
	"default": {
		"run_type": "EXECUTABLE_ZIP",
		"windows": {
			"artifactName": "UnityClient@Windows.zip",
			"command": "./UnityClient@Windows.exe",
			"arguments": [
				"+appName",
				"${IMPROBABLE_PROJECT_NAME}",
				"+localAssetDatabasePath",
				"${IMPROBABLE_PROJECT_ROOT}/build/assembly",
				"+useInternalIpForBridge",
				"true",
				"+linkProtocol",
				"RakNet"
			]
		}
	}
  }
}